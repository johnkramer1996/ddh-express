{
  "name": "ddd",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "db": "",
    "db:create:dev": "ts-node scripts/db/create",
    "db:create:test": "cross-env NODE_ENV=test ts-node scripts/db/create",
    "db:delete:dev": "ts-node scripts/db/delete",
    "db:delete:test": "cross-env NODE_ENV=test ts-node scripts/db/delete",
    "db:flush:dev": "npm run db:delete:dev && npm run db:create:dev",
    "db:flush:test": "npm run db:delete:test && npm run db:create:test",
    "db:start:dev": "npm run migrate:flush:dev && npm run seed:flush:dev",
    "migrate:flush:dev": "npm run migrate:down:dev && npm run migrate:up:dev",
    "migrate:up:dev": "sequelize db:migrate",
    "migrate:down:dev": "sequelize db:migrate:undo:all",
    "migrate:test": "cross-env NODE_ENV=test sequelize db:migrate --env test",
    "migrate:prod": "cross-env NODE_ENV=production sequelize db:migrate --env production",
    "seed:flush:dev": "npm run seed:down:dev && npm run seed:up:dev",
    "seed:up:dev": "sequelize db:seed:all",
    "seed:down:dev": "sequelize db:seed:undo:all",
    "seed:up:test": "sequelize db:seed:all",
    "setup": "",
    "setup:dev": "npm install && npm run db:create:dev && npm run migrate:dev && cd public/app && npm install",
    "start:dev": "cross-env NODE_ENV=development nodemon",
    "start": "nodemon",
    "test": "jest",
    "test:dev": "jest --watchAll"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "dompurify": "^3.0.6",
    "dotenv": "^16.3.1",
    "env-var": "^7.4.1",
    "express": "^4.18.2",
    "inversify": "^6.0.1",
    "jsdom": "^22.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.33.0",
    "sequelize-cli": "^6.6.1",
    "sequelize-typescript": "^2.1.5",
    "slug": "^8.2.3",
    "sqlite3": "^5.1.6",
    "supertest": "^6.3.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.4",
    "@types/dompurify": "^3.0.3",
    "@types/express": "^4.17.18",
    "@types/jest": "^29.5.5",
    "@types/jsdom": "^21.1.3",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/morgan": "^1.9.6",
    "@types/pg": "^8.10.3",
    "@types/redis": "^4.0.11",
    "@types/slug": "^5.0.5",
    "@types/supertest": "^2.0.13",
    "@types/uuid": "^9.0.4",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsconfig-paths": "^4.2.0"
  }
}
