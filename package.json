{
  "name": "ddd",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "db": "",
    "db:create:dev": "ts-node scripts/db/create",
    "db:create:test": "cross-env NODE_ENV=test ts-node scripts/db/create",
    "db:delete:dev": "ts-node scripts/db/delete",
    "db:delete:test": "cross-env NODE_ENV=test ts-node scripts/db/delete",
    "db:flush:dev": "npm run db:delete:dev && npm run db:create:dev",
    "db:flush:test": "npm run db:delete:test && npm run db:create:test",
    "migrate": "",
    "migrate:dev": "sequelize db:migrate",
    "migrate:dev:down": "sequelize db:migrate:undo",
    "migrate:test": "cross-env NODE_ENV=test sequelize db:migrate --env test",
    "migrate:prod": "cross-env NODE_ENV=production sequelize db:migrate --env production",
    "seed:dev": "sequelize db:seed:undo:all && sequelize db:seed:all",
    "seed:test": "sequelize db:seed:all",
    "setup": "",
    "setup:dev": "npm install && npm run db:create:dev && npm run migrate:dev && cd public/app && npm install",
    "start:dev": "cross-env NODE_ENV=development nodemon",
    "start": "nodemon",
    "test": "jest",
    "test:dev": "jest --watchAll"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.3.1",
    "env-var": "^7.4.1",
    "express": "^4.18.2",
    "inversify": "^6.0.1",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.33.0",
    "sequelize-cli": "^6.6.1",
    "sequelize-typescript": "^2.1.5",
    "supertest": "^6.3.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.18",
    "@types/jest": "^29.5.5",
    "@types/morgan": "^1.9.6",
    "@types/pg": "^8.10.3",
    "@types/supertest": "^2.0.13",
    "@types/uuid": "^9.0.4",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsconfig-paths": "^4.2.0"
  }
}
